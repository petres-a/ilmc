{
  "name": "IntraILoveMyCity",
  "description": "Intranet City ILoveMyCity",
  "author": "Quentin Jaccarino <jaccarino.quentin@gmail.com>",
  "version": "0.1.0",
  "keywords": [
    "React"
  ],
  "main": "server.js",
  "scripts": {
    "start": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "lint": "standard",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "npm run lint"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "aphrodite": "0.4.1",
    "axios": "0.12.0",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-react-display-name": "6.8.0",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "babel-runtime": "6.9.2",
    "better-npm-run": "0.0.9",
    "bluebird": "3.4.1",
    "body-parser": "1.15.2",
    "clean-webpack-plugin": "0.1.9",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "draft-js": "0.7.0",
    "express": "4.14.0",
    "file-loader": "0.9.0",
    "helmet": "2.1.1",
    "js-cookie": "2.1.2",
    "keycode": "2.1.2",
    "lodash.throttle": "4.0.1",
    "material-ui": "0.15.2",
    "moment": "2.14.1",
    "morgan": "1.7.0",
    "ms": "0.7.1",
    "pretty-error": "2.0.0",
    "react": "15.2.1",
    "react-addons-shallow-compare": "15.2.1",
    "react-collapse": "2.3.1",
    "react-dom": "15.2.1",
    "react-dropzone": "3.5.3",
    "react-google-maps": "4.11.0",
    "react-height": "2.1.1",
    "react-helmet": "3.1.0",
    "react-infinite": "0.9.2",
    "react-motion": "0.4.4",
    "react-redux": "4.4.5",
    "react-router": "3.0.0-alpha.1",
    "react-router-redux": "4.0.5",
    "react-tap-event-plugin": "1.0.0",
    "redial": "0.4.2",
    "redux": "3.5.2",
    "serialize-javascript": "1.3.0",
    "serve-favicon": "2.3.0",
    "socket.io-client": "1.4.8",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-isomorphic-tools": "2.3.2"
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "babel-plugin-react-transform": "2.0.2",
    "concurrently": "2.2.0",
    "piping": "0.3.0",
    "react-transform-render-visualizer": "0.4.0",
    "standard": "7.1.2",
    "standard-loader": "4.0.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.1"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "add-module-exports",
      "transform-react-display-name"
    ]
  }
}
